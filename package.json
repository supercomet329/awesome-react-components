{
  "name": "hazel-ui",
  "version": "0.0.8",
  "private": false,
  "bugs": "https://github.com/hazel-ui/hazel-ui/issues",
  "homepage": "https://hazel-ui.github.io/hazel-ui",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hazel-ui/hazel-ui.git"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o ./dist",
    "serve-storybook": "npx http-server ./dist",
    "prettier": "prettier --check \"{.storybook,public,src}/**/*.{js,jsx,ts,tsx,json,yml,md,mdx}\"",
    "prettier:fix": "prettier --write \"{.storybook,public,src}/**/*.{js,jsx,ts,tsx,json,yml,md,mdx}\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@bbc/gel-foundations": "^5.0.1",
    "react-table": "^7.5.2",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/node-logger": "^6.0.27",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.27",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.67",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-table": "^7.0.24",
    "@types/styled-components": "^5.1.3",
    "@types/styled-system": "^5.1.10",
    "babel-loader": "^8.1.0",
    "gh-pages": "^3.1.0",
    "prettier": "2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "react-scripts": "4.0.0",
    "typescript": "^3.9.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "ui",
    "library",
    "design-system",
    "react-components",
    "components",
    "styled-components"
  ]
}
